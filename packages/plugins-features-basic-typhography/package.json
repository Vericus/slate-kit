{
  "version": "0.1.11",
  "name": "@vericus/slate-kit-basic-typhography",
  "description": "plugin that provides basic typography functionality to slate",
  "homepage": "https://github.com/Vericus/slate-kit/",
  "repository": "git://github.com/Vericus/slate-kit.git",
  "author": "Cadmus",
  "slate-kit": {
    "type": "module"
  },
  "files": [
    "dist/",
    "lib/",
    "module/"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/slate-kit-basic-typhography.js",
  "types": "lib/index.d.ts",
  "module": "lib/slate-kit-basic-typhography.es.js",
  "umd": "dist/slate-kit-basic-typhography.js",
  "umdMin": "dist/slate-kit-basic-typhography.min.js",
  "scripts": {
    "clean": "yarn run clean:module && yarn run clean:build",
    "clean:module": "rm -rf ./node_modules",
    "clean:build": "rm -rf ./dist ./lib ./module",
    "build": "tsc -b .",
    "dev": "tsc -w"
  },
  "umdGlobals": {
    "immutable": "Immutable",
    "tslib": "TSLib",
    "slate": "Slate",
    "is-plain-obj": "IsPlainObj",
    "@vericus/slate-kit-utils-register-helpers": "SlateKitRegister",
    "@vericus/slate-kit-basic-typography-renderer": "TypographyRenderer",
    "@vericus/slate-kit-plugins-utils": "SlateKitPluginsutils"
  },
  "dependencies": {
    "@vericus/slate-kit-basic-typography-renderer": "^0.1.10",
    "@vericus/slate-kit-utils-register-helpers": "^0.0.1",
    "debug": "^3.1.0",
    "immutable": ">=3.8.2",
    "is-plain-obj": "^1.1.0",
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@vericus/slate-kit-typescript-typings": "^1.0.11",
    "expect": "^23.1.0",
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0",
    "slate": ">=0.44.0 <0.45.0",
    "slate-simulator": ">=0.4.59",
    "to-camel-case": "^1.0.0"
  },
  "peerDependencies": {
    "@vericus/slate-kit-basic-typography-renderer": "^0.1.0",
    "@vericus/slate-kit-plugins-utils": "^0.1.0",
    "@vericus/slate-kit-utils-register-helpers": "^0.0.1",
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0",
    "slate": ">=0.44.0 <0.45.0"
  },
  "keywords": [
    "Vericus",
    "slate",
    "slate-kit",
    "slate-plugin",
    "editor",
    "typography"
  ]
}
